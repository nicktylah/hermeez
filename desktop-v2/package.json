{
  "name": "hermes",
  "version": "1.0.0",
  "description": "Desktop client for Hermes",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack-cli --progress --config webpack.config.prod.js",
    "build-staging": "NODE_ENV=staging webpack-cli --progress --config webpack.config.staging.js",
    "clean": "rm -rf ./build/",
    "deploy": "npm run build && NODE_ENV=production node ./scripts/deploy.js",
    "deploy-staging": "npm run build-staging && NODE_ENV=staging node ./scripts/deploy-staging.js",
    "format": "./node_modules/.bin/prettier --write --single-quote \"{src,scripts}/**/*.{js,jsx}\"",
    "format:check": "./node_modules/.bin/prettier -l --single-quote \"{src,scripts}/**/*.{js,jsx}\"",
    "prebuild": "npm run clean",
    "prebuild-staging": "npm run clean",
    "start": "NODE_ENV=development node ./server.js",
    "flow": "flow",
    "flow:watch": "chokidar 'src/**' -c 'flow' --initial",
    "test": "npm run flow && npm run format:check && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicktylah/hermeez.git"
  },
  "author": "nicktylah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nicktylah/hermeez/issues"
  },
  "homepage": "https://github.com/nicktylah/hermeez#readme",
  "devDependencies": {
    "aws-sdk": "2.203.0",
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "0.28.7",
    "flow-bin": "0.61.0",
    "flow-webpack-plugin": "1.2.0",
    "html-webpack-plugin": "3.2.0",
    "less": "3.0.4",
    "less-loader": "4.1.0",
    "node-sass": "4.7.2",
    "prettier": "1.9.2",
    "redux-logger": "3.0.6",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "webpack": "4.0.1",
    "webpack-cli": "2.0.9",
    "webpack-dev-server": "2.9.7"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "date-fns": "1.29.0",
    "firebase": "4.10.1",
    "flow": "0.2.3",
    "fuse.js": "3.2.1",
    "google-libphonenumber": "3.1.1",
    "lodash": "4.17.4",
    "query-string": "5.0.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "3.1.3",
    "react-redux": "5.0.6",
    "react-reveal": "1.2.2",
    "redux": "3.7.2",
    "redux-saga": "1.1.3",
    "reselect": "3.0.1"
  }
}
