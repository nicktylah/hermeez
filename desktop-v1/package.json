{
  "name": "Hermes",
  "private": false,
  "version": "0.0.9",
  "description": "UI for sending and receiving SMS/MMS messages with an Android phone",
  "main": "main.js",
  "scripts": {
    "audit": "npm run test && npm run lint",
    "clean": "rm -rf ./tmp ./build",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js --progress --colors",
    "build-dev": "NODE_ENV=development ./node_modules/webpack/bin/webpack.js --config webpack.config.dev.js --watch --progress --colors",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint ./src --ext js,jsx",
    "test": "mocha-webpack \"src/components/**/_tests/*.spec.js\" --webpack-config webpack.config.test.js",
    "package-mac": "electron-packager ./build --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/hermes_1024.icns --prune=true --out=release-builds",
    "ship": "npm run build && npm run package-mac"
  },
  "devDependencies": {
    "babel-core": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.11.5",
    "chai": "3.5.0",
    "classnames": "2.2.5",
    "color": "0.11.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "0.23.1",
    "eslint": "3.1.1",
    "eslint-config-standard": "5.3.5",
    "eslint-config-standard-jsx": "2.0.0",
    "eslint-config-standard-react": "3.0.0",
    "eslint-plugin-promise": "2.0.0",
    "eslint-plugin-react": "5.2.2",
    "eslint-plugin-standard": "2.0.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "flow-bin": "0.29.0",
    "jsdom": "9.4.1",
    "json-loader": "0.5.4",
    "lodash": "4.14.0",
    "mocha": "2.5.3",
    "mocha-webpack": "0.5.0",
    "node-sass": "3.8.0",
    "react": "15.2.1",
    "react-addons-css-transition-group": "15.2.1",
    "react-addons-test-utils": "15.2.1",
    "react-dom": "15.2.1",
    "react-hot-loader": "3.0.0-beta.2",
    "react-redux": "4.4.5",
    "react-router": "2.6.0",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "sass-loader": "3.2.1",
    "semistandard": "8.0.0",
    "sinon": "1.17.4",
    "style-loader": "0.13.1",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^0.5.9",
    "webpack": "^2.5.1",
    "webpack-dev-server": "1.14.1",
    "webpack-node-externals": "1.3.0"
  },
  "dependencies": {
    "@google-cloud/storage": "^1.1.1",
    "crypto-js": "^3.1.9-1",
    "electron": "^1.6.2",
    "electron-config": "^1.0.0",
    "firebase": "^4.1.3",
    "google-cloud": "^0.53.0",
    "lodash": "^4.14.0",
    "moment": "^2.15.2",
    "pako": "^1.0.5",
    "react": "^15.2.1",
    "react-loading": "0.0.9",
    "request": "^2.74.0",
    "request-promise": "^4.1.1"
  }
}
